{
    "name": "cline/keyphrase",
    "description": "A modern, fluent API for generating secure passwords, passphrases, and mnemonics with BIP39 and EFF dice wordlists.",
    "license": "MIT",
    "keywords": [
        "php",
        "laravel",
        "password",
        "passphrase",
        "mnemonic",
        "bip39",
        "eff",
        "diceware",
        "security"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/contracts": "^11.0 || ^12.0",
        "illuminate/support": "^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.92.7"
    },
    "require-dev": {
        "cline/coding-standard": "^1.5.6",
        "orchestra/testbench": "^10.8",
        "symfony/var-dumper": "^7.4.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cline\\Keyphrase\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cline\\Keyphrase\\KeyphraseServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "vendor/bin/ecs check --fix",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:lint": "vendor/bin/ecs check",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "pest --parallel --exactly=100 --type-coverage",
        "test:types": "phpstan",
        "test:unit": "pest --parallel --exactly=100 --coverage"
    }
}
